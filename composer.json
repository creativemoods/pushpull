    "keywords": ["wordpress", "plugin"],
    "homepage": "https://creativemoods.pt",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "CreativeMoods\\PushPull\\": "inc/",
            "CreativeMoods\\PushPullTests\\": "tests/phpunit/Tests/"
        }
